  "bugs": {
    "url": "https://github.com/ericmdantas/html-paths/issues"
  },
  "homepage": "https://github.com/ericmdantas/html-paths#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.15",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.2.0",
    "sinon": "^1.17.6"
  }
}
